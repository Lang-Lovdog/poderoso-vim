{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "liotree",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_line"
              },
              {
                "type": "STRING",
                "value": "\n"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_line"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "root_line"
        },
        {
          "type": "SYMBOL",
          "name": "structure_line"
        },
        {
          "type": "SYMBOL",
          "name": "comment_line"
        },
        {
          "type": "SYMBOL",
          "name": "blank_line"
        }
      ]
    },
    "blank_line": {
      "type": "PATTERN",
      "value": "[ \\t]*"
    },
    "root_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[ \\t]*"
        },
        {
          "type": "SYMBOL",
          "name": "lateral_bar"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "name"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "inline_comment"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "structure_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[ \\t]*"
        },
        {
          "type": "SYMBOL",
          "name": "lateral_bar"
        },
        {
          "type": "FIELD",
          "name": "depth",
          "content": {
            "type": "SYMBOL",
            "name": "depth_marks"
          }
        },
        {
          "type": "PATTERN",
          "value": "[ \\t]+"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "name"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "inline_comment"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "comment_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[ \\t]*"
        },
        {
          "type": "SYMBOL",
          "name": "lateral_bar"
        },
        {
          "type": "PATTERN",
          "value": "[ \\t]{2,}"
        },
        {
          "type": "FIELD",
          "name": "text",
          "content": {
            "type": "SYMBOL",
            "name": "comment_text"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "comment_end"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "name": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "quoted_dirname"
        },
        {
          "type": "SYMBOL",
          "name": "quoted_filename"
        },
        {
          "type": "SYMBOL",
          "name": "unquoted_dirname"
        },
        {
          "type": "SYMBOL",
          "name": "unquoted_filename"
        },
        {
          "type": "SYMBOL",
          "name": "unclosed_quoted_string"
        }
      ]
    },
    "lateral_bar": {
      "type": "STRING",
      "value": "|"
    },
    "depth_marks": {
      "type": "PATTERN",
      "value": "[-+]+"
    },
    "inline_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\s*"
        },
        {
          "type": "SYMBOL",
          "name": "comment_start"
        },
        {
          "type": "FIELD",
          "name": "text",
          "content": {
            "type": "SYMBOL",
            "name": "comment_text"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "comment_end"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "comment_start": {
      "type": "STRING",
      "value": ">"
    },
    "comment_end": {
      "type": "STRING",
      "value": "||"
    },
    "comment_text": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^|\\n]*"
      }
    },
    "quoted_dirname": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "'"
              },
              {
                "type": "REPEAT",
                "content": {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "PATTERN",
                      "value": "[^']"
                    },
                    {
                      "type": "PATTERN",
                      "value": "\\\\."
                    }
                  ]
                }
              },
              {
                "type": "STRING",
                "value": "'/"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "\""
              },
              {
                "type": "REPEAT",
                "content": {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "PATTERN",
                      "value": "[^\"]"
                    },
                    {
                      "type": "PATTERN",
                      "value": "\\\\."
                    }
                  ]
                }
              },
              {
                "type": "STRING",
                "value": "\"/"
              }
            ]
          }
        ]
      }
    },
    "quoted_filename": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "'"
              },
              {
                "type": "REPEAT",
                "content": {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "PATTERN",
                      "value": "[^']"
                    },
                    {
                      "type": "PATTERN",
                      "value": "\\\\."
                    }
                  ]
                }
              },
              {
                "type": "STRING",
                "value": "'"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "\""
              },
              {
                "type": "REPEAT",
                "content": {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "PATTERN",
                      "value": "[^\"]"
                    },
                    {
                      "type": "PATTERN",
                      "value": "\\\\."
                    }
                  ]
                }
              },
              {
                "type": "STRING",
                "value": "\""
              }
            ]
          }
        ]
      }
    },
    "unquoted_dirname": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^-+|>\\s][^|>]*\\/"
      }
    },
    "unquoted_filename": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^-+|>\\s][^|>]*"
      }
    },
    "unclosed_quoted_string": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "'"
              },
              {
                "type": "REPEAT",
                "content": {
                  "type": "PATTERN",
                  "value": "[^'\\n]"
                }
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "\""
              },
              {
                "type": "REPEAT",
                "content": {
                  "type": "PATTERN",
                  "value": "[^\"\\n]"
                }
              }
            ]
          }
        ]
      }
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}