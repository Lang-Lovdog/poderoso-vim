{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "liotree",
      "camelcase": "LioTree",
      "title": "Lovdog Directory Tree Format",
      "scope": "source.liotree",
      "file-types": [
        "liotree"
      ],
      "injection-regex": "^liotree$",
      "class-name": "TreeSitterLiotree"
    }
  ],
  "metadata": {
    "version": "0.0.1",
    "license": "MIT",
    "description": "Directory tree format for visual ASCII tree description and directory tree structure creation",
    "authors": [
